# Docker Compose Environment Variables
# Copy this file to .env and update with your actual values

# =============================================================================
# API Keys (Required)
# =============================================================================
OPENAI_API_KEY=your_openai_api_key_here
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# =============================================================================
# TradingAgents Configuration
# =============================================================================
TRADINGAGENTS_LLM_PROVIDER=openai
TRADINGAGENTS_DEEP_THINK_LLM=o4-mini
TRADINGAGENTS_QUICK_THINK_LLM=gpt-4o-mini
TRADINGAGENTS_RESULTS_DIR=/app/results

# =============================================================================
# Database Configuration
# =============================================================================
POSTGRES_DB=tradingagents
POSTGRES_USER=tradingagents
POSTGRES_PASSWORD=changeme_secure_password
POSTGRES_PORT=5432
DATABASE_ECHO=false

# =============================================================================
# Redis Configuration
# =============================================================================
REDIS_ENABLED=true
REDIS_PASSWORD=changeme_secure_password
REDIS_DB=0
REDIS_PORT=6379
REDIS_MAX_MEMORY=256mb

# =============================================================================
# Streaming and Workers Configuration
# =============================================================================
STREAMING_ENABLED=true
AUTO_START_WORKERS=true
WORKER_MODE=data
WORKER_CONCURRENCY=4

# =============================================================================
# Monitoring Configuration
# =============================================================================
MONITORING_ENABLED=true
METRICS_ENABLED=true

# =============================================================================
# Watchdog Configuration
# =============================================================================
WATCHDOG_ENABLED=true
WATCHDOG_CHECK_INTERVAL=60
WATCHDOG_TASK_TIMEOUT=300

# =============================================================================
# Application Settings
# =============================================================================
DEBUG=false

# =============================================================================
# Service Ports
# =============================================================================
BACKEND_PORT=8000
FRONTEND_PORT=3000
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443

# =============================================================================
# Frontend Configuration
# =============================================================================
NEXT_PUBLIC_API_URL=http://localhost:8000
