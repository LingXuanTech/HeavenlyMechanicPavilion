{
  "name": "@tradingagents/shared",
  "version": "0.1.0",
  "private": true,
  "description": "Shared TypeScript utilities, domain models, and generated API clients for TradingAgents.",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./theme": {
      "import": "./src/theme/index.ts",
      "require": "./src/theme/index.ts",
      "types": "./src/theme/index.ts"
    },
    "./theme/*": {
      "import": "./src/theme/*.ts",
      "require": "./src/theme/*.ts",
      "types": "./src/theme/*.ts"
    },
    "./domain": {
      "import": "./src/domain/index.ts",
      "require": "./src/domain/index.ts",
      "types": "./src/domain/index.ts"
    },
    "./domain/*": {
      "import": "./src/domain/*.ts",
      "require": "./src/domain/*.ts",
      "types": "./src/domain/*.ts"
    },
    "./clients": {
      "import": "./src/clients/index.ts",
      "require": "./src/clients/index.ts",
      "types": "./src/clients/index.ts"
    },
    "./clients/*": {
      "import": "./src/clients/*.ts",
      "require": "./src/clients/*.ts",
      "types": "./src/clients/*.ts"
    },
    "./utils": {
      "import": "./src/utils/index.ts",
      "require": "./src/utils/index.ts",
      "types": "./src/utils/index.ts"
    },
    "./utils/*": {
      "import": "./src/utils/*.ts",
      "require": "./src/utils/*.ts",
      "types": "./src/utils/*.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json"
  },
  "devDependencies": {
    "typescript": "^5.4.0"
  }
}
